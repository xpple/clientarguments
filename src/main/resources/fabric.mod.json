{
  "schemaVersion": 1,
  "id": "clientarguments",
  "version": "${version}",
  "name": "clientarguments",
  "description": "Quality of life library for client-sided Minecraft Fabric command building.",
  "authors": [
    "xpple"
  ],
  "contact": {
    "website": "https://xpple.dev/",
    "repo": "https://github.com/xpple/ClientArguments"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.xpple.clientarguments.ClientArguments"
    ]
  },
  "depends": {
    "fabricloader": ">={fabric_loader_version}",
    "fabric-command-api-v2": "*",
    "minecraft": "{minecraft_version_dependency}"
  },
  "accessWidener": "clientarguments.aw",
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  }
}
